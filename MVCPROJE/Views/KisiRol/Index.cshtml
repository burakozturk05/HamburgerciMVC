@using MVCPROJE.Areas.Identity.Data;
@using Microsoft.AspNetCore.Identity
@model List<Kullanici>

<h1> ROLLER Listesi</h1>

<table class="table table-hover">
	<tr>
	<thead>
		<tr>
			<td>Kullanici Adı</td>
			<td></td>
			<td></td>
		</tr>

	</thead>
	<tbody>
			@{
				foreach (var item in Model)
				{
				<tr>

					<td>@item.UserName</td>
					<td><a class="btn btn-info" asp-action="Edit" asp-route-id="@item.Id">DÜZENLE</a></td>
					<td><a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.Id">Sil</a></td>
				</tr>
				}
			}
	</tbody>


</table>